<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/pimple_recycler"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        style="@style/heading_report_style"
        android:id="@+id/typical_work_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/typical_work_text"/>
    <EditText
        style="@style/edit_text_report_style"
        android:id="@+id/typical_work_edit_text"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/typical_work_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/typical_work_edit_text"
        app:endIconMode="clear_text"/>

</androidx.constraintlayout.widget.ConstraintLayout>